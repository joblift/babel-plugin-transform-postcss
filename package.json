{
  "name": "@joblift/babel-plugin-transform-postcss",
  "version": "0.2.4",
  "description": "PostCSS Babel Transform",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && flow check && BABEL_ENV=coverage nyc mocha",
    "build": "babel src --copy-files --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joblift/babel-plugin-transform-postcss"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "coverage/**",
      "dist/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": false,
    "instrument": false,
    "temp-directory (pending-accepted-pr)": "coverage/nyc"
  },
  "author": "Whitney Young",
  "license": "MIT",
  "keywords": [
    "postcss",
    "postcss-modules",
    "babel",
    "babel-transform",
    "css-modules"
  ],
  "homepage": "https://github.com/joblift/babel-plugin-transform-postcss",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-env": "^1.1.8",
    "babel-register": "^6.18.0",
    "chai": "^4.0.2",
    "chai-string": "^1.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "flow-bin": "^0.164.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "postcss": "^8.3.11",
    "postcss-modules": "^4.2.2",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "string-hash": "^1.1.1"
  },
  "peerDependencies": {
    "postcss": "^6.0.6 || ^7.0.0 || ^8.0.0"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "postcss-load-config": "^2.0.0"
  }
}
